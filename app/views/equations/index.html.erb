<h1>All Equations</h1>

<table>
	<tbody>
	<% @equations.each do |equation| %>
		<%= content_tag_for(:tr, equation) do %> 
			<td><%= image_tag equation.image_file %></td>
			<td><%= link_to(equation.latex, equation_path(equation)) %></td>
			<td><%= link_to("Edit", edit_equation_path(equation)) %></td>
		<% end %>
	<% end %>
	</tbody>
</table>
